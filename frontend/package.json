{
  "name": "llm-inference-frontend",
  "version": "1.0.0",
  "description": "LLM推理服务前端界面",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "dev:desktop": "vite --host 0.0.0.0 --port 3000 --mode desktop",
    "dev:mobile": "vite --host 0.0.0.0 --port 3000 --mode mobile",
    "build": "vue-tsc --noEmit && vite build",
    "build:pwa": "vue-tsc --noEmit && vite build --mode pwa",
    "build:mobile": "vue-tsc --noEmit && vite build --mode mobile",
    "build:desktop": "vue-tsc --noEmit && vite build --mode desktop",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "preview:pwa": "vite preview --host 0.0.0.0 --port 4173 --mode pwa",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.3.8",
    "pinia": "^2.1.7",
    "framework7": "^8.3.1",
    "framework7-vue": "^8.3.1",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "vue-chartjs": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "typescript": "~5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
